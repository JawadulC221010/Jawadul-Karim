#include <stdio.h>
 
int main() {
    int n;
    int i,c=0,b=0,d=0,f=0;
    for(i=0;i<5;i++)
    {
    scanf("%d",&n);
        if(n>0)
        c++;
        if(n<0)
        b++;
        if(n%2==0)
        d++;
        if(n%2!=0)
        f++;
    }
    printf("%d valor(es) par(es)\n",d);
    printf("%d valor(es) impar(es)\n",f);
    printf("%d valor(es) positivo(s)\n",c);
    printf("%d valor(es) negativo(s)\n",b);
    
    return 0;
}

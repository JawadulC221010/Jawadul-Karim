#include <stdio.h>
#include <math.h>

int main() {
    double a,b,c,x,y;
    scanf("%lf %lf %lf",&a,&b,&c);
    
    if ((((b*b)-(4*a*c))<0)||(a==0))
    printf("Impossivel calcular");
    else
{
    x=(-b+(sqrt((b*b)-(4*a*c))))/(2.0*a);
    y=(-b-(sqrt((b*b)-(4*a*c))))/(2.0*a);
    printf("R1 = %.5lf\nR2 = %.5lf",x,y);
}
    return 0;
}
